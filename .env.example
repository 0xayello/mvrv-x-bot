# Twitter API (required)
TWITTER_API_KEY=
TWITTER_API_SECRET=
TWITTER_ACCESS_TOKEN=
TWITTER_ACCESS_SECRET=

# Optional: Chart screenshot fallback
# If not set, code will try PUBLIC_BASE_URL or VERCEL_URL (injected by Vercel)
PUBLIC_BASE_URL=
SCREENSHOT_PROVIDER=urlbox
URLBOX_API_KEY=
SCREENSHOTONE_KEY=

# Optional: Santiment API (only if SantimentService is used)
SANTIMENT_API_KEY=

# Note: VERCEL_URL is provided automatically by Vercel in runtime
